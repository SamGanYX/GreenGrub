create database GreenGrub;

use GreenGrub;

DROP TABLE users;

CREATE TABLE "users" (
  "id" bigint NOT NULL AUTO_INCREMENT,
  "email" varchar(255) DEFAULT NULL,
  "full_name" varchar(255) DEFAULT NULL,
  "username" varchar(255) DEFAULT NULL,
  PRIMARY KEY ("id")
);

