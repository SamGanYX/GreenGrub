create database GreenGrub;

use GreenGrub;

DROP TABLE users;

CREATE TABLE
  "users" (
    "id" bigint NOT NULL AUTO_INCREMENT,
    "username" varchar(255) UNIQUE DEFAULT NULL,
    "password" varchar(255) DEFAULT NULL,
    PRIMARY KEY ("id")
  );

CREATE TABLE "barcodes" (
  "id" bigint NOT NULL AUTO_INCREMENT,
  "user_id" bigint,
  "barcode" varchar(255),
  "active" boolean,
  FOREIGN KEY ("user_id") REFERENCES "users"("id"),
  PRIMARY KEY ("id")
)